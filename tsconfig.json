{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "declaration": true,
    "baseUrl": "./src",
    "module": "ES2022",
    "moduleResolution": "Node",
    "emitDeclarationOnly": true,
    "outDir": "node_modules/.cache/tsbuild",
    "paths": {}
  }
}
